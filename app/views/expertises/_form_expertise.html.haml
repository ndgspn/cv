= form_for @expertise do | expertise |
  .form-group
    = expertise.label :name
    = expertise.text_field :name, class: 'form-control'

  .form-group
    = expertise.label :level
    = expertise.text_field :level, class: 'form-control'

  .form-group
    = expertise.submit 'submit', class: 'btn btn-primary'
